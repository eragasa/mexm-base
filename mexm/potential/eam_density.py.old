from mexm.potential import Potential
from mexm.potential import MEXM_GLOBAL_FMT
from mexm.potential import MEXM_1BODY_FMT
from mexm.potential import MEXM_2BODY_FMT
from mexm.potential import MEXM_3BODY_FMT
from mexm.potential import MEXM_HYBRID_GLOBAL_FMT
from mexm.potential import MEXM_HYBRID_1BODY_FMT
from mexm.potential import MEXM_HYBRID_2BODY_FMT

class EamDensityFunction(Potential):
    potential_type = 'eam_density'
    is_base_potential = True
    """implements the base class for the EAM Density Function"""
def __init__(self,
                 symbols):

        Potential.__init__(self,
                symbols=symbols,
                potential_type=potential_type)

        self.density_evaluations = None
